<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE doc [
<!ENTITY structureDefs SYSTEM "src/test/resources/test/structureDefs.xmlFragment">
<!ENTITY caseFileModel SYSTEM "src/test/resources/test/caseFileModel.xmlFragment">
<!ENTITY lifecycleTestTasks SYSTEM "src/test/resources/test/lifecycleTestTasks.xmlFragment">
]>
<cmmn:definitions xmlns:cmmn="http://www.omg.org/spec/CMMN/20121031/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:test="http://ifu-lobuntu.org/test"
	targetNamespace="http://ifu-lobuntu.org/test">
	
	&structureDefs;
	<cmmn:case name="CaseInstanceTests" id="CaseInstanceTests">
		&caseFileModel;
		<cmmn:casePlanModel name="CaseInstanceTests" exitCriteriaRefs="waitingForEndUserEventSentryId" autoComplete="false">
			<cmmn:planningTable id="theRootPlanningTableId" authorizedRoleRefs="constructionProjectManagerRoleId">
			</cmmn:planningTable>
			<cmmn:planItem definitionRef="startUserEventId" name="StartUserEventPlanItem" id="startUserEventPlanItemId" />
			<cmmn:planItem definitionRef="endUserEventId" name="EndUserEventPlanItem" id="endUserEventPlanItemId" />
			<cmmn:planItem definitionRef="theCaseTaskId" name="TheCaseTaskPlanItem" id="theCaseTaskPlanItemId" entryCriteriaRefs="waitingForStartUserEventSentryId1" />
			<cmmn:planItem definitionRef="theHumanTaskId" name="TheHumanTaskPlanItem" id="theHumanTaskPlanItemId" entryCriteriaRefs="waitingForStartUserEventSentryId2" />
			<cmmn:planItem definitionRef="theMilestoneId" name="TheMilestonePlanItem" id="theMilestonePlanItemId" entryCriteriaRefs="waitingForStartUserEventSentryId3" />
			<cmmn:planItem definitionRef="theTimerEventId" name="TheTimerEventPlanItem" id="theTimerEventPlanItemId" />
			<cmmn:planItem definitionRef="theUserEventId" name="TheUserEventPlanItem" id="theUserEventPlanItemId" />
			<cmmn:caseTask id="theCaseTaskId" name="TheCaseTask" caseRef="SubCase.cmmn#SubCase" isBlocking="true">
				<cmmn:inputs bindingRef="housePlanFileItemId" id="housePlanTaskParameter" name="housePlanTaskParameter">
				</cmmn:inputs>
				<cmmn:inputs bindingRef="wallPlanFileItemId" id="wallPlanTaskParameter" name="wallPlanTaskParameter">
				</cmmn:inputs>
				<cmmn:parameterMapping sourceRef="housePlanTaskParameter" targetRef="SubCase.cmmn#housePlanSubCaseParameterId" />
				<cmmn:parameterMapping sourceRef="wallPlanTaskParameter" targetRef="SubCase.cmmn#wallPlanSubCaseParameterId">
				</cmmn:parameterMapping>
			</cmmn:caseTask>
			<cmmn:humanTask id="theHumanTaskId" name="TheHumanTask" performerRef="builderRoleId"></cmmn:humanTask>
			<cmmn:milestone id="theMilestoneId" name="TheMilestone" />
			<cmmn:timerEvent id="theTimerEventId" name="TheTimerEvent">
				<cmmn:timerExpression language="http://www.mvel.org/mvel">
					<cmmn:body>1h</cmmn:body>
				</cmmn:timerExpression>
			</cmmn:timerEvent>
			<cmmn:userEvent id="theUserEventId" name="TheUserEvent" />
			<cmmn:userEvent id="startUserEventId" name="StartUserEvent" />
			<cmmn:sentry id="waitingForStartUserEventSentryId1" name="WaitingForStartUserEventSentry1">
				<cmmn:planItemOnPart id="onStartUserEventOccurred" sourceRef="startUserEventPlanItemId">
					<cmmn:standardEvent>occur</cmmn:standardEvent>
				</cmmn:planItemOnPart>
			</cmmn:sentry>
			<cmmn:sentry id="waitingForStartUserEventSentryId2" name="WaitingForStartUserEventSentry2">
				<cmmn:planItemOnPart id="onStartUserEventOccurred" sourceRef="startUserEventPlanItemId">
					<cmmn:standardEvent>occur</cmmn:standardEvent>
				</cmmn:planItemOnPart>
			</cmmn:sentry>
			<cmmn:sentry id="waitingForStartUserEventSentryId3" name="WaitingForStartUserEventSentry3">
				<cmmn:planItemOnPart id="onStartUserEventOccurred" sourceRef="startUserEventPlanItemId">
					<cmmn:standardEvent>occur</cmmn:standardEvent>
				</cmmn:planItemOnPart>
			</cmmn:sentry>

			<cmmn:userEvent id="endUserEventId" name="EndUserEvent" />
			<cmmn:sentry id="waitingForEndUserEventSentryId" name="WaitingForEndUserEventSentry">
				<cmmn:planItemOnPart id="onEndUserEventOccurred" sourceRef="endUserEventPlanItemId">
					<cmmn:standardEvent>occur</cmmn:standardEvent>
				</cmmn:planItemOnPart>
			</cmmn:sentry>
		</cmmn:casePlanModel>
		<cmmn:caseRoles name="Builders" id="builderRoleId" />
		<cmmn:caseRoles name="ConstructionProjectManagers" id="constructionProjectManagerRoleId" />
		<cmmn:input bindingRef="housePlanFileItemId" id="housePlanInputId" name="housePlan" />
		<cmmn:input bindingRef="houseFileItemId" id="houseInputId" name="house" />
	</cmmn:case>
</cmmn:definitions>

	   