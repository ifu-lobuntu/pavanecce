<t='http://www.ifu-lobuntu.org/testing'>
<mix='http://www.jcp.org/jcr/mix/1.0'>


[t:roofPlan] > mix:lifecycle, mix:referenceable

[t:wall] > mix:lifecycle, mix:referenceable

[t:walls] 
	+ t:wall (t:wall) = t:wall multiple

[t:house] > mix:lifecycle, mix:referenceable
	- t:description (string)
	- t:wallPlans (reference) multiple < t:wallPlan
	- t:roofPlan (reference) < t:roofPlan
	+ t:walls (t:walls) = t:walls

[t:wallPlan] > mix:lifecycle, mix:referenceable
	- t:wall (reference) < t:wall

[t:wallPlans] 
	+ t:wallPlan (t:wallPlan) = t:wallPlan multiple

[t:housePlan] > mix:lifecycle, mix:referenceable
	+ t:roofPlan (t:roofPlan)
	+ t:wallPlans (t:wallPlans) = t:wallPlans

[t:constructionCase] > mix:lifecycle, mix:referenceable
	- t:name (string)
	- t:startDate (date)
	+ t:housePlan (t:housePlan)
	+ t:house (t:house) 
