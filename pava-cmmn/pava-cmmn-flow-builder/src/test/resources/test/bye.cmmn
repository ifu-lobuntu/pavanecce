<?xml version="1.0" encoding="UTF-8"?>
<cmmn:definitions xmlns:cmmn="http://www.omg.org/spec/CMMN/20121031/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.omg.org/spec/CMMN/20121031/MODEL CMMN10CaseModel.xsd" targetNamespace="http:///hello">
	<cmmn:caseFileItemDefinition id="housePlanDefinition" definitionType="http://www.omg.org/spec/CMMN/DefinitionType/UMLClass"
		structureRef="file:///model.uml#org/pavanecce/cmmn/HousePlan" />
	<cmmn:caseFileItemDefinition id="roofPlanDefinitionId" definitionType="http://www.omg.org/spec/CMMN/DefinitionType/UMLClass"
		structureRef="file:///model.uml#org/pavanecce/cmmn/WallPlan" />
	<cmmn:caseFileItemDefinition id="wallPlanDefinitionId" definitionType="http://www.omg.org/spec/CMMN/DefinitionType/UMLClass"
		structureRef="file:///model.uml#org/pavanecce/cmmn/RoofPlan" />
	<cmmn:caseFileItemDefinition id="wallQuoteDefinition" definitionType="http://www.omg.org/spec/CMMN/DefinitionType/UMLClass"
		structureRef="file:///model.uml#org/pavanecce/cmmn/WallQuote" />
	<cmmn:caseFileItemDefinition id="wallDefinitionId" definitionType="http://www.omg.org/spec/CMMN/DefinitionType/UMLClass"
		structureRef="file:///model.uml#org/pavanecce/cmmn/Wall">
	</cmmn:caseFileItemDefinition>
	<cmmn:caseFileItemDefinition id="houseDefinitionId" definitionType="http://www.omg.org/spec/CMMN/DefinitionType/UMLClass"
		structureRef="file:///model.uml#org/pavanecce/cmmn/House">
	</cmmn:caseFileItemDefinition>
	<cmmn:case name="bye" id="bye">
		<cmmn:caseFileModel>
			<cmmn:caseFileItem name="housePlan" id="housePlanFileItemId" multiplicity="ExactlyOne" definitionRef="housePlanDefinition">
				<cmmn:children>
					<cmmn:caseFileItem name="wallPlans" id="wallPlanFileItemId" multiplicity="OneOrMore" definitionRef="wallPlanDefinitionId" targetRefs="wallFileItemId">
						<cmmn:children>
							<cmmn:caseFileItem name="wallQuoteId" definitionRef="wallQuoteDefinition" multiplicity="ExactlyOne" id="wallQuoteItem"></cmmn:caseFileItem>
						</cmmn:children>
					</cmmn:caseFileItem>
					<cmmn:caseFileItem name="roofPlan" id="roofPlanFileItemId" multiplicity="ZeroOrOne" definitionRef="roofPlanDefinitionId">
					</cmmn:caseFileItem>
				</cmmn:children>
			</cmmn:caseFileItem>
			<cmmn:caseFileItem name="house" id="houseFileItemId" multiplicity="ExactlyOne" definitionRef="houseDefinitionId">
				<cmmn:children>
					<cmmn:caseFileItem name="walls" id="wallFileItemId" multiplicity="OneOrMore" definitionRef="wallDefinitionId" />
				</cmmn:children>
			</cmmn:caseFileItem>
		</cmmn:caseFileModel>
		<cmmn:casePlanModel name="BuildIt">
			<cmmn:planItem definitionRef="buildWallId" name="BuildWallPlanItem" id="buildWallPlanItemId" entryCriteriaRefs="wallplanCreatedSentryId"
				exitCriteriaRefs="waitingForWallSentryId">
			</cmmn:planItem>
			<cmmn:planItem definitionRef="buildRoofID" name="BuildRoofPlanItem" id="paintWallPlanItemId" entryCriteriaRefs="waitingForWallAndRoofSentryId">
			</cmmn:planItem>
			<cmmn:sentry id="waitingForWallAndRoofSentryId" name="WaitingForWallSentry">
				<cmmn:caseFileItemOnPart sourceRef="roofPlanFileItemId" id="OnRoofPlanCreatePart">
					<cmmn:standardEvent>create</cmmn:standardEvent>
				</cmmn:caseFileItemOnPart>
				<cmmn:caseFileItemOnPart sourceRef="wallFileItemId" id="OnWallCreatePart">
					<cmmn:standardEvent>create</cmmn:standardEvent>
				</cmmn:caseFileItemOnPart>
			</cmmn:sentry>
			<cmmn:sentry id="waitingForWallSentryId" name="WaitingForWallSentry">
				<cmmn:caseFileItemOnPart sourceRef="wallFileItemId" id="OnWallCreatePart">
					<cmmn:standardEvent>create</cmmn:standardEvent>
				</cmmn:caseFileItemOnPart>
			</cmmn:sentry>
			<cmmn:sentry id="wallplanCreatedSentryId" name="WaitingForWallPlanCreatedSentry">
				<cmmn:caseFileItemOnPart sourceRef="wallPlanFileItemId" id="OnWallPlanCreatePart">
					<cmmn:standardEvent>create</cmmn:standardEvent>
				</cmmn:caseFileItemOnPart>
			</cmmn:sentry>
			<cmmn:humanTask id="buildWallId" name="Build Wall" performerRef="builderRoleId" isBlocking="true">
				<cmmn:defaultControl />
				<cmmn:inputs name="wallPlan" bindingRef="wallPlanFileItemId"></cmmn:inputs>
				<cmmn:outputs name="wall" bindingRef="wallFileItemId"></cmmn:outputs>
			</cmmn:humanTask>
			<cmmn:humanTask id="buildRoofID" name="Build Roof" performerRef="builderRoleId" isBlocking="true">
				<cmmn:defaultControl />
				<cmmn:inputs name="roofPlan" bindingRef="roofPlanFileItemId"></cmmn:inputs>
			</cmmn:humanTask>
		</cmmn:casePlanModel>
		<cmmn:caseRoles name="Painter" id="painterRoleId" />
		<cmmn:caseRoles name="Builder" id="builderRoleId" />
		<cmmn:input bindingRef="housePlanFileItemId" id="housePlanInputId" name="housePlan">
		</cmmn:input>
		<cmmn:input bindingRef="houseFileItemId" id="houseInputId" name="house">
		</cmmn:input>

	</cmmn:case>
</cmmn:definitions>

	   